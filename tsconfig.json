{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES5",
    "module": "CommonJS",
    "outDir": "./build",
    "rootDir": "./src",
    "moduleResolution": "node",
    // "lib": ["ES2015", "DOM", "DOM.Iterable"],
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "skipLibCheck": true
  },
  "include": ["./src/**/*", "src/handler.ts"],
  "exclude": ["node_modules", "dist"]
}
